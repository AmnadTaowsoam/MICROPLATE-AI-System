{
  "name": "prediction-db-service",
  "version": "1.0.0",
  "description": "Database service for managing prediction data with Prisma",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "ts-node src/seed.ts",
    "db:reset": "prisma migrate reset --force --skip-seed",
    "db:seed": "npm run prisma:seed",
    "db:studio": "prisma studio",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "fastify",
    "prisma",
    "typescript",
    "microservice",
    "prediction",
    "database"
  ],
  "author": "Microplate AI Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "redis": "^4.6.10",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2",
    "prisma": "^5.7.1"
  }
}